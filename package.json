    "@types/dompurify": "^3.0.5",
    "@types/qrcode": "^1.5.5",
    "apexcharts": "^4.0.0",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "country-list": "^2.3.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "eslint": "^8.57.0",
    "framer-motion": "^12.23.0",
    "gsap": "^3.13.0",
    "hint.css": "^3.0.0",
