{
  "name": "velocards-dashboard",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "VeloCards",
    "email": "support@velocards.com",
    "url": "https://velocards.com"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "test -d ../shared || git clone https://$GITHUB_TOKEN@github.com/velocards/velocards_shared.git ../shared && cd ../shared && npm install && npm run build && cd -",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out node_modules",
    "clean:install": "npm run clean && npm install --legacy-peer-deps",
    "production": "npm run build && npm run start"
  },
  "dependencies": {
